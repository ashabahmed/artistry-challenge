<h1>Create Artists</h1>

<% if flash[:errors]%>
<%flash[:errors].each do |error|%>
<%= error%>
<%end%>
<%end%>

<%=form_for @artist do |f| %>
<%= f.label :name%>
<%= f.text_field :name%>
<br>

<%= f.label :title%>
<%= f.text_field :title%>
<br>
<%= f.label :age%>
<%= f.text_field :age%>
<%= f.submit%>
<%end%>
